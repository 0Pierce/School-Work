<Window x:Class="A1_AWS_S3.ObjectOps"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:A1_AWS_S3"
        mc:Ignorable="d"
        Title="ObjectOps" Height="500" Width="800">
    <Grid>
        <Button x:Name="btnBack" Content="Back to Main Window" HorizontalAlignment="Center" Margin="0,420,0,0" VerticalAlignment="Top" Height="33" Width="452" Click="btnBack_Click"/>
        <Label Content="Bucket:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontSize="16"/>
        <ComboBox x:Name="dbObject" HorizontalAlignment="Left" Margin="94,10,0,0" VerticalAlignment="Top" Width="353" Height="31" FontSize="16" SelectionChanged="dbObject_SelectionChanged"/>
        <DataGrid x:Name="objectDataGrid" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,96,174,69"/>
        <Label Content="Object" HorizontalAlignment="Left" Margin="10,46,0,0" VerticalAlignment="Top" Height="33" Width="61" FontSize="16"/>
        <TextBox x:Name="tbObject" HorizontalAlignment="Left" Margin="94,54,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="353" Height="25"/>
        <Button x:Name="btnBrowse" Content="Browse" HorizontalAlignment="Left" Margin="661,96,0,0" VerticalAlignment="Top" Height="47" Width="114" Click="btnBrowse_Click"/>
        <Button x:Name="btnUpload" Content="Upload" HorizontalAlignment="Left" Margin="661,172,0,0" VerticalAlignment="Top" Height="47" Width="114" Click="btnUpload_Click"/>
    </Grid>
</Window>
